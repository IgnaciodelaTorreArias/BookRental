{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2",
      "ClientCertificateMode": "AllowCertificate"
    },
    "Endpoints": {
      "Inventory": {
        "Url": "https://0.0.0.0:5501",
        "Certificate": {
          "Path": "certificate.pfx",
          "Password": "${CERT_PASSWORD}"
        }
      }
    }
  }
}
