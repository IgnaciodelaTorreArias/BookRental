{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "APIGateway": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "APIGateway": {
        "Url": "https://0.0.0.0:5500",
        "ClientCertificateMode": "AllowCertificate",
        "Certificate": {
          "Path": "APIGateway.pfx"
        }
      }
    }
  }
}
