{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "APIGateway": {
        "Url": "https://0.0.0.0:5500",
        "ClientCertificateMode": "AllowCertificate",
        "Certificate": {
          "Path": "certificate.pfx",
          "Password": "${CERT_PASSWORD}"
        }
      }
    }
  }
}
